{
  "name": "react-boilerplate-component",
  "version": "3.2.2",
  "homepage": "./",
  "private": true,
  "dependencies": {
    "@beingenious/jsonpointer": "^1.0.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "firebase": "9.23.0",
    "formik": "^2.1.4",
    "idb": "^8.0.0",
    "lodash": "^4.17.19",
    "pandasuite-bridge": "^4.3.3",
    "pandasuite-bridge-react": "^2.3.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-app-polyfill": "^1.0.6",
    "react-dom": "^16.13.0",
    "react-intl": "^3.12.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "tabler-react": "^1.30.1",
    "yup": "^0.28.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "package-session": "cd src/json/session && cp ../../../pandasuite-component.zip ../../../pandasuite-session-component.zip && zip -ur ../../../pandasuite-session-component.zip pandasuite.json",
    "package-auth": "cd src/json/auth && cp ../../../pandasuite-component.zip ../../../pandasuite-auth-component.zip && zip -ur ../../../pandasuite-auth-component.zip pandasuite.json",
    "build-generic": "react-scripts build && cd build && zip -r -X '../pandasuite-component.zip' *",
    "build": "yarn clean && yarn build-generic && yarn package-auth && yarn package-session",
    "clean": "rm -rf build && rm -f pandasuite-component.zip && rm -f pandasuite-auth-component.zip && rm -f pandasuite-session-component.zip",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "release": "yarn build && release-it --no-npm.publish"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "IE >= 9",
      "Android > 0",
      "iOS > 0"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "release-it": "^13.0.1"
  },
  "release-it": {
    "github": {
      "release": true,
      "assets": [
        "pandasuite-auth-component.zip",
        "pandasuite-session-component.zip"
      ]
    }
  }
}
